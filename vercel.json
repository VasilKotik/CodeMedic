{
  "version": 2,
  "builds": [
    {
      "src": "api/ai-request.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ai-request",
      "dest": "/api/ai-request.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, X-Internal-Token"
        }
      ]
    }
  ]
}

